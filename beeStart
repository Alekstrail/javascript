<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title>canvas</title>
</head>
<body>
	<canvas id="canvas" width="300" height="300"></canvas>
	
	<script>
	var canvas = document.getElementById("canvas");
	var ctx = canvas.getContext("2d");
	
	var circle =function (x,y,redius,fillCircle) {
	ctx.beginPath();
	ctx.arc(x,y,redius,0,Math.PI*2,false);
	if (fillCircle) {
	ctx.fill();
	} else {
	ctx.stroke();
	}
	};
	</script>
</body>
</html>
