<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title>canvas</title>
</head>
<body>
	<canvas id="canvas" width="300" height="300"></canvas>
	
	<script>
	var canvas = document.getElementById("canvas");
	var ctx = canvas.getContext("2d");
	ctx.fillStyle = "Yellow";
		ctx.fillRect (100,10,50,50);
		ctx.fillRect (120,60,10,30);
		ctx.fillRect (60,90,140,20);
		ctx.fillRect (90,110,80,80);
		ctx.strokeStyle = "Pink";
		ctx.lineWidth = 6;
		ctx.strokeRect (90,190,20,70);
		ctx.strokeRect (150,190,20,70);
		ctx.fillStyle = "Red";
	
	</script>
</body>
</html>
